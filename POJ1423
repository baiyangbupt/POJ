#include <iostream>
#include<cmath>
using namespace std;

int dp[10000001];

void init(){
    dp[1] = 1;
    double sum = 0;
    for(int i = 2; i <= 10000000; i++){
        sum+=log10((double)i);
        dp[i] = (int)sum+1;
    }
}

int main(int argc, const char * argv[])
{
    int nc,n;
    init();
    cin >> nc;
    while(nc--){
        cin >> n;
        cout << dp[n] << endl;
    }

    return 0;
}

