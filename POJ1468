#include <iostream>
using namespace std;

int n;
struct rec{
    int x1, x2, y1, y2;
}rr[5500];

bool cover(rec &r1, rec &r2){
    return (r1.x1>=r2.x1&&r1.x2<=r2.x2&&r1.y1>=r2.y1&&r1.y2>=r2.y2);
}

int main(int argc, const char * argv[])
{
    while(cin>>n){
        for(int i = 0; i < n; i++){
            cin >> rr[i].x1 >> rr[i].x2 >> rr[i].y1 >> rr[i].y2;
        }
        int res = 0;
        for(int i = 0; i < n; i++)
            for(int j = 0; j < n; j++)
                if(j!=i&&cover(rr[i],rr[j])){
                    res++;
                    break;
                }
        cout << res << endl;
    }
    return 0;
}

