#include <iostream>
using namespace std;

int adj[21][21];
const int inf = 50;

void floyd(){
    for(int k = 1; k <= 20; k++)
        for(int i = 1; i <= 20; i++)
            for(int j = 1; j <= 20; j++)
                if(adj[i][k]+adj[k][j]<adj[i][j])
                    adj[i][j] = adj[j][i] = adj[i][k]+adj[k][j];
}

int main(int argc, const char * argv[])
{
    int n;
    int nc = 1;
    while(cin>>n){
        for(int i = 1; i <= 20; i++)
            for(int j = i+1; j <= 20; j++)
                adj[i][j] = adj[j][i] = inf;
        for(int i = 0; i < n; i++){
            int t;
            cin >> t;
            adj[1][t] = adj[t][1] = 1;
        }
        
        for(int i = 2; i < 20; i++){
            cin >> n;
            for(int j = 0; j < n; j++){
                int t;
                cin >> t;
                adj[i][t] = adj[t][i] =1;
            }
        }
        
        floyd();
        cin >> n;
        cout << "Test Set #" << nc++ << endl;
        for(int i = 0; i < n; i++){
            int u,v;
            cin >> u >> v;
            cout << u << " to " << v << ": " << adj[u][v] << endl;
        }
        cout << endl;
    }
    return 0;
}

