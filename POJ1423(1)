#include <iostream>
#include <cmath>
using namespace std;

int main(int argc, const char * argv[])
{
    int nc;
    cin >> nc;
    const double PI = acos(-1.0);
    double c2=0.434294481903;
    while(nc--){
        int n;
        cin >> n;
        if(n==1){
            cout << 1 << endl;
            continue;
        }
        int res = 0.5*log10(2*PI*n)+n*log10((double)n)-n*c2+1;
        cout <<res << endl;
    }

    return 0;
}

