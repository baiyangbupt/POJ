#include <iostream>
using namespace std;

bool adj[150][150], visit[150];
int match[150],nxt[150], n, num;


bool dfs(int p){
    for(int i = 1; i <= n; i++)
        if(adj[p][i]&&visit[i]==0){
            visit[i] = 1;
            if(match[i]==-1||dfs(match[i])){
                match[i] = p;
                nxt[p] = i;
                return 1;
            }
        }
    return 0;
}

int main(int argc, const char * argv[])
{
    int nc;
    cin >> nc;
    while(nc--){
        memset(adj,0,sizeof(adj));
        memset(match,-1,sizeof(match));
        memset(nxt,-1,sizeof(nxt));
        num = 0;
        int e;
        cin >> n >> e;
        for(int i = 0; i < e; i++){
            int x,y;
            cin >> x >> y;
            adj[x][y] = 1;
        }
        for(int i = 1; i <= n; i++)
            if(nxt[i]<0){
                memset(visit,0,sizeof(visit));
                if(dfs(i))
                    num++;
            }
        cout << n-num << endl;
        
    }
    return 0;
}

