#include <iostream>
using namespace std;

int main(int argc, const char * argv[])
{
    int nc,sum,n;
    cin >> nc;
    while(nc--){
        cin >> n;
        sum = 0;
        while(n/=5)
            sum+=n;
        cout << sum << endl;
    }
    return 0;
}

